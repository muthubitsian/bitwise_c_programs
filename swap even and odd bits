#include <stdio.h>

void swap_e_o_bits(int x)
{
    
    int even_bits=x&0x55555555;
    int odd_bits=x&0xAAAAAAAA;
    even_bits=even_bits<<1;
    odd_bits=odd_bits>>1;
    printf("%d",even_bits|odd_bits);
}


int main()
{
    printf("Hello World");
    swap_e_o_bits(23);
    return 0;
}
