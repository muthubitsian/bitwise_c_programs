#include<stdio.h>

#include<math.h>
int pow_t(int n,int x)
{
    
    int result=x;
    for(int i=1;i<=n;i++)
    {
        result=result*x;
    }
    return result;
    
    
}
int main()
{
    int n;
    int i=0;
    printf("enter the number");
    scanf("%d",&n);
    
    for(i=0;i>=0;i++)
    {
        if(pow_t(i,2)>=n)
        break;
    }
    printf("%d",pow(2,i));
    
    
    return 0;
    
}
